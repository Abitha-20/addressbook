pipeline {
    agent any

    stages {
        stage('Compile') {
            steps {
                script{
                     echo 'COMPIEL-Hello World'
                }
               
            }
        }
         stage('UnitTest') {
            steps {
                script{
                     echo 'UNITTEST-Hello World'
                }
               
            }
        }
         stage('Package') {
            steps {
                script{
                     echo 'PACKAGE-Hello World'
                }
               
            }
        }
    }

}
